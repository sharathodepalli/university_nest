# UniNest Environment Variables Template
# Copy this file to .env.local and update with your actual values
# NEVER commit .env files with real values to git!

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# SendGrid Configuration for Email Verification
SENDGRID_API_KEY=SG.your_sendgrid_api_key_here
VITE_FROM_EMAIL=noreply@yourdomain.com

# Optional: App Configuration
VITE_APP_NAME=UniNest
VITE_SUPPORT_EMAIL=support@yourdomain.com
VITE_TEST_EMAIL=your-test-email@example.com

# Optional: Google Maps API (for address validation)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Environment
NODE_ENV=development
